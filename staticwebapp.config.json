{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": [
      "*.{png,jpg,jpeg,gif,svg}",
      "*.css",
      "*.js",
      "*.map",
      "*.xml",
      "*.json"
    ]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/404.html",
      "statusCode": 404
    }
  },
  "mimeTypes": {
    ".svg": "image/svg+xml"
  }
}
